{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/[[...route]]"
    },
    {
      "source": "/:path*",
      "destination": "/api/[[...route]]"
    }
  ]
}
